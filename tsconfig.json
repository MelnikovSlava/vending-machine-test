{
  "compilerOptions": {
    "jsx": "react",
    "module": "none",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "allowJs": false,
    "experimentalDecorators": true,
  },
  "exclude": [
    "node_modules"
  ]
}